package progavanzada.model;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "heroes")
public class Heroes {
	@Id
	int id;
	String nombre;
	String rango;
	int habilidad;
	int derrotados;
	int fans;
	int residencia;
	int telefono;
	String agencia;
}
